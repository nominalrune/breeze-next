{
    "compilerOptions": {
        "baseUrl": "src/",
        "paths": {
            "@/*": [
                "./*"
            ]
        },
        "target": "ESNext",
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "allowJs": false,
        "skipLibCheck": true,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsxdev",
        "importsNotUsedAsValues": "error"
    },
    "include": [
        "./src"
    ]
}
